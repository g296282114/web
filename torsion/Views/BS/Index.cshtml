            <a href="index.html#" data-original-title="Go to Home"><i href="www.baidu.com" class="icon-remove">
            </i>Close</a> <a href="index.html#" class="current">Dashboard x</a>
        </div>
        <div class="container-fluid">
            <div class="row-fluid">
                <div id="glftabs" class="easyui-tabs" data-options="tools:'#tab-tools'" style="width: 100%;">
                    
                </div>
                <div id="footer" class="span12">
                    2012 © Unicorn Admin. Brought to you by <a href="https://wrapbootstrap.com/user/diablo9983">
                        diablo9983</a>
                </div>
            </div>
        </div>
    </div>
    <ul class="typeahead dropdown-menu">
    </ul>
    <script type="text/javascript">
     
        function wresize() {
            var ti = 130;
            if ($(window).width() < 479) {
                ti = 200;
            }

            $('#glftabs').tabs({
                height: $(window).height() - ti
            });
        }
        $(window).resize(wresize);
        wresize();
//        function iFrameHeight(idname) {
//            alert($(window).height());
//            return;
//            var thisheight = $(document).height() + 30; //获取页面高度

//            $("#" + idname).height(thisheight);
//        }
        function glf_newtab(mainmenu, submenu) {
            var tid = "glftabs_" + mainmenu + '_' + submenu;
            var ttid = "#" + tid;
            var iframeurl;
            switch (mainmenu) {
                case 1:
                    {
                        iframeurl = "home";
                        break;
                    }
                case 2:
                    {
                        iframeurl = "attendance/test"
                    break;
                }
            case 3:
                {

                    iframeurl = "home/test"
                    break;
                }
                    default:
                    {
                    iframeurl = "http://www.baidu.com"
                    }
            }
            $('#glftabs').tabs('add', {
                title: 'glftabs_' + mainmenu + '_' + submenu,
                content: '<iframe id=' + tid + '  scrolling=auto  frameborder="0" src="' + iframeurl + '" style="width: 100%;height:100%;margin-bottom : -10px;"></iframe>',
                closable: true,
                selected: true
            });
           $('#glftabs').tabs('getTab',tid).panel('options').tab.hide();
        }

        function seltab(mainmenu, submenu) {
            var tid = "glftabs_" + mainmenu + '_' + submenu;
            if (!$('#glftabs').tabs('exists',tid)) {
                glf_newtab(mainmenu,submenu);
            }
            else
                $("#glftabs").tabs("select", tid);

        }
    </script>
</body>
